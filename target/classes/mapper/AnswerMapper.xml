<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dong.mingjiuzhang.mapper.AnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dong.mingjiuzhang.domain.entity.Answer">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="subject_dict_id" property="subjectDictId" />
        <result column="subject_first_dict_id" property="subjectFirstDictId" />
        <result column="subject_second_dict_id" property="subjectSecondDictId" />
        <result column="subject_cate_id" property="subjectCateId" />
        <result column="activity_id" property="activityId" />
        <result column="answer_duration" property="answerDuration" />
        <result column="status" property="status" />
        <result column="is_deleted" property="isDeleted" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, subject_dict_id, subject_first_dict_id, subject_second_dict_id, subject_cate_id, activity_id, answer_duration, status, is_deleted, gmt_create, gmt_modified
    </sql>

</mapper>
